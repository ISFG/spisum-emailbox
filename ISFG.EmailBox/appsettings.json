{
  "SpisUmEmailConfiguration": {
    "AutomaticResponse": {
      "SendResponse": true,
      "BodyTextFile": {
        "Folder": "ConfigurationFiles",
        "FileName" :  "EmailAutomaticResponse.txt"
      },
      "EmailSubject": "Email subject",
      "OrganizationName": "Organzation name"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "TestConfig": {
    "Test1": "Yep"
  },
  "Alfresco": {
    "Url": "http://hostname.domain:8082",
    "TokenExpire": 30,
    "SystemUser": {
      "Username": "emailbox",
      "Password": "emailbox"
    }
  }
}
